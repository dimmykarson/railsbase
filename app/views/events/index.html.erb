<table id="example" class="table table-striped table-bordered dt-responsive nowrap base-responsive" cellspacing="0" width="100%">

  <thead>
  <tr>
    <th>Data</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @events.each do |event| %>
      <tr>
        <td><%= event.data %></td>
        <td><%= event.title %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<div class="text-right">
  <%= paginate @events %>
</div>
<div class="text-right">
  <%= link_to 'New Event', new_event_path %>
</div>
